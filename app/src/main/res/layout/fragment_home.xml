<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Complementario"
    tools:context=".Proyecto.HomeFragment">

    <!-- TODO: Update blank fragment layout -->


    <ImageView
        android:id="@+id/ivConsejoHome"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:src="@drawable/actividad"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.768"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.587" />

    <ImageView
        android:id="@+id/ivActividadHome"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:src="@drawable/actividad"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.271"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.198" />

    <TextView
        android:id="@+id/tvActividadHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivActividadHome"
        app:layout_constraintStart_toEndOf="@+id/ivActividadHome"
        app:layout_constraintTop_toTopOf="@+id/ivActividadHome" />

    <TextView
        android:id="@+id/tvConsejoHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivConsejoHome"
        app:layout_constraintEnd_toStartOf="@+id/ivConsejoHome"
        app:layout_constraintTop_toTopOf="@+id/ivConsejoHome" />

</androidx.constraintlayout.widget.ConstraintLayout>