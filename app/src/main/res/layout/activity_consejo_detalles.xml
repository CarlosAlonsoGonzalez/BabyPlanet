<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Proyecto.ConsejoDetalles">

    <TextView
        android:id="@+id/tvTipoConsejo"
        android:layout_width="265dp"
        android:layout_height="58dp"
        app:layout_constraintBottom_toTopOf="@+id/tvNombreConsejo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/tvNombreConsejo"
        android:layout_width="180dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toTopOf="@+id/tvDes"
        app:layout_constraintTop_toBottomOf="@+id/tvTipoConsejo"
        app:layout_constraintVertical_bias="0.5"
        tools:layout_editor_absoluteX="2dp" />

    <TextView
        android:id="@+id/tvDes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DescripciÃ³n:"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/tvTextoDescripcion"
        app:layout_constraintTop_toBottomOf="@+id/tvNombreConsejo"
        app:layout_constraintVertical_bias="0.5"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/tvTextoDescripcion"
        android:layout_width="248dp"
        android:layout_height="148dp"
        android:layout_marginBottom="120dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvDes"
        app:layout_constraintVertical_bias="0.5"
        tools:layout_editor_absoluteX="73dp" />
</androidx.constraintlayout.widget.ConstraintLayout>